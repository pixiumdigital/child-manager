{
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "transformIgnorePatterns": ["^.+\\.js$"],
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "moduleDirectories": ["node_modules", "src"],
  "modulePathIgnorePatterns": ["dashboard"]
}
